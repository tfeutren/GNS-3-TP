            ipv6 rip 1 enable
            exit
        interface GigabitEthernet3/0
            no shutdown
            ipv6 enable
            ipv6 address 2020:0:1:23::2/64
            ipv6 rip 1 enable
            exit
        interface loopback 0
            no shutdown
            ipv6 enable
            ipv6 address 1::2/128
            ipv6 rip 1 enable
            exit
end

