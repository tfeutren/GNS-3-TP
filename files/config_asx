R1

enable
    clear bgp ipv6 unicast *
    configure terminal
        ipv6 unicast-routing
        ipv6 router rip 1
            redistribute connected
            exit
        router bgp 1
            no bgp default ipv4-unicast
            bgp router-id 1.1.1.1
            neighbor 1::6 remote-as 1
            neighbor 1::6 update-source loopback 0 
            neighbor 1::7 remote-as 1
            neighbor 1::7 update-source loopback 0 
            address-family ipv6 unicast
                neighbor 1::6 activate 
                neighbor 1::7 activate
                exit
            exit
        interface GigabitEthernet1/0
            no shutdown
            ipv6 enable
            ipv6 address 2020:0:1:12::1/64
            ipv6 rip 1 enable
            exit
        interface GigabitEthernet2/0
            no shutdown
            ipv6 enable
            ipv6 address 2020:0:1:13::1/64
            ipv6 rip 1 enable
            exit
        interface loopback 0
            no shutdown
            ipv6 enable
            ipv6 address 1::1/128
            ipv6 rip 1 enable
            exit
end

R2

enable
    clear bgp ipv6 unicast *
    configure terminal
        ipv6 unicast-routing
        ipv6 router rip 1
            redistribute connected
            exit
        router bgp 1
            no bgp default ipv4-unicast
            bgp router-id 2.2.2.2
            neighbor 1::6 remote-as 1
            neighbor 1::6 update-source loopback 0 
            neighbor 1::7 remote-as 1
            neighbor 1::7 update-source loopback 0 
            address-family ipv6 unicast
                neighbor 1::6 activate 
                neighbor 1::7 activate
                exit
            exit
        interface GigabitEthernet1/0
            no shutdown
            ipv6 enable
            ipv6 address 2020:0:1:12::2/64
            ipv6 rip 1 enable
            exit
        interface GigabitEthernet2/0
            no shutdown
